<view class="container"><view class="login_group"><image class="background_img" src="../../static/img/group.png"></image><block wx:if="{{isLogin}}"><view class="user_info"><view class="user_avatar"><open-data type="userAvatarUrl"></open-data></view><text>{{userInfo.name}}</text><view class="points" hidden="{{true}}" data-event-opts="{{[['tap',[['goToIntegral',['$event']]]]]}}" bindtap="__e">{{"积分 "+individualPoints.points+" >"}}</view></view></block><block wx:else><view class="user_info"><view class="user_avatar"><image src="../../static/img/account.png" onerror="getDefaultImg"></image></view><text data-event-opts="{{[['tap',[['onTapLogin',['$event']]]]]}}" bindtap="__e">点击登录</text></view></block></view><block wx:if="{{isLogin}}"><view class="perosn_info"><view class="info_item"><image class="person_pic" src="../../static/img/personPic1.png"></image><view><text>个人信息</text><text class="text_bottom">设置个人信息</text></view><image class="person_arrow" src="../../static/img/33.png"></image></view><view class="info_item"><image class="person_pic" src="../../static/img/personPic2.png"></image><view><text>个人资质</text><text class="text_bottom">上传个人资质</text></view><image class="person_arrow" src="../../static/img/33.png"></image></view></view></block><block wx:if="{{isLogin}}"><view class="task_status"><view data-event-opts="{{[['tap',[['toBeConfirm',['$event']]]]]}}" class="task_item" bindtap="__e"><image src="../../static/img/tobeconfirm.png"></image><view>待确认</view><uni-badge vue-id="1" text="2" type="error" size="small" bind:__l="__l"></uni-badge></view><view data-event-opts="{{[['tap',[['toMountGuard',['$event']]]]]}}" class="task_item" bindtap="__e"><image src="../../static/img/tobemount.png"></image><view style="margin-top:8rpx;">待上岗</view><uni-badge vue-id="2" text="2" type="error" size="small" bind:__l="__l"></uni-badge></view><view data-event-opts="{{[['tap',[['toBeCompleted',['$event']]]]]}}" class="task_item" bindtap="__e"><image src="../../static/img/tobefinish.png"></image><view>待完成</view><uni-badge vue-id="3" text="2" type="error" size="small" bind:__l="__l"></uni-badge></view><view data-event-opts="{{[['tap',[['toBeFinished',['$event']]]]]}}" class="task_item" bindtap="__e"><image src="../../static/img/finished.png"></image><view>已完成</view></view></view></block><block wx:if="{{isLogin}}"><view class="list"><uni-list vue-id="4" bind:__l="__l" vue-slots="{{['default']}}"><uni-list-item vue-id="{{('5')+','+('4')}}" title="我的钱包" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('6')+','+('4')}}" title="我的银行卡" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('7')+','+('4')}}" title="我的店铺" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('8')+','+('4')}}" title="我的协议" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('9')+','+('4')}}" title="我的消息" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('10')+','+('4')}}" title="设置" show-arrow="{{true}}" bind:__l="__l"></uni-list-item><uni-list-item vue-id="{{('11')+','+('4')}}" title="分享二维码" show-arrow="{{true}}" bind:__l="__l"></uni-list-item></uni-list></view></block></view>