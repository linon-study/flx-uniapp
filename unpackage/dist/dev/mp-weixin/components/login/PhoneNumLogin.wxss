.phone_group {
  padding: 200rpx 0 0 0;
}
.phone_group .uni-form-item {
  border-bottom: 1px solid #dddddd;
}
.phone_group .uni-form-item text {
  float: left;
  font-size: 32rpx;
  color: #333333;
  padding-left: 20rpx;
  padding-right: 20rpx;
  line-height: 88rpx;
}
.phone_group .uni-form-item input {
  font-size: 16px;
  width: 400rpx;
  height: 88rpx;
  line-height: 88rpx;
}
.phone_group .uni-form-item button {
  float: right;
  color: #519ff0;
  font-size: 26rpx;
  margin: 10rpx;
}
.phone_group .uni-btn-v button {
  margin-top: 144rpx;
  line-height: 80rpx;
  height: 80rpx;
  width: 80%;
  text-align: center;
  background: #519ff0;
  color: #fff;
}

